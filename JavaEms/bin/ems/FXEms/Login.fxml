            <Text fx:id="LoginTitle" strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome! Please Login." textAlignment="CENTER" wrappingWidth="400.0" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
